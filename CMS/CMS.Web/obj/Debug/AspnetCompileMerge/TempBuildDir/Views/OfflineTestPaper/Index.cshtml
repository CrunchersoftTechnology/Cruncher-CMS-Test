@model IEnumerable<CMS.Web.ViewModels.OfflineTestPaperViewModel>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>Exam Schedule List</h2>

<p>
    @Html.ActionLink("Create New Exam Schedule", "Create")
</p>
<div class="panel panel-default">
    <div class="panel-body">

        @Html.Partial("_MVCGridToolbar", new CMS.Web.Models.MVCGridToolbarModel()
   {
       MVCGridName = "OfflineTestPaperGrid",
       PageSize = true,
       ColumnVisibility = true,
       Export = true,
       GlobalSearch = false
   })
        @Html.MVCGrid("OfflineTestPaperGrid", new { userRole = ViewBag.userId })
    </div>
</div>


