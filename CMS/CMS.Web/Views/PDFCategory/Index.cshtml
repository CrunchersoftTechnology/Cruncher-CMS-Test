@model IEnumerable<CMS.Web.ViewModels.PDFCategoryViewModel>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<br />

<h2>Upload Category List</h2>
<p>
    @Html.ActionLink("Create New PDF Category", "Create")
</p>

<div class="panel panel-default">
    <div class="panel-body">
        @Html.Partial("_MVCGridToolbar", new CMS.Web.Models.MVCGridToolbarModel()
   {
       MVCGridName = "PDFCategoryGrid",
       PageSize = true,
       ColumnVisibility = true,
       Export = true,
       GlobalSearch = false
   })
        @Html.MVCGrid("PDFCategoryGrid")
    </div>
</div>
