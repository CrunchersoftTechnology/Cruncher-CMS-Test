@model IEnumerable<CMS.Web.ViewModels.UploadOfflineMarksViewModel>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>Upload Offline Marks List</h2>

<p>
    @Html.ActionLink("Upload Offline Marks", "Create")
</p>
<div class="panel panel-default">
    <div class="panel-body">

        @Html.Partial("_MVCGridToolbar", new CMS.Web.Models.MVCGridToolbarModel()
   {
       MVCGridName = "OfflineTestPaperStudentMarksGrid",
       PageSize = true,
       ColumnVisibility = true,
       Export = true,
       GlobalSearch = false
   })
        @Html.MVCGrid("OfflineTestPaperStudentMarksGrid", new { userRole = ViewBag.userId })
    </div>
</div>